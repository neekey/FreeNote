<div id="J_note-form" class="note-form note-form-hide" >
    <p class="note-content-con">
        <textarea type="text" placeholder="note" id="J_note-content" class="note-content"></textarea>
    </p>
    <p class="note-tag-con">
        <input type="text" placeholder="tag" id="J_note-tag" class="note-tag" />
    </p>
    <p class="note-btn">
        <input type="button" value="添加" id="J_note-add" class="note-add" />
        <input type="button" value="取消" id="J_note-cancel class="note-cancel" />
        <input type="button" value="关闭" id="J_note-close class="note-close" />

    </p>
</div>